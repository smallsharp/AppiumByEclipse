<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Play" preserve-order="true" >
    
    <test name="测试登录" preserve-order="true">

        <classes>
            <class name="com.cmall.testcases.TestCase_Model" > 
                <methods>
                <parameter name="mobile" value="18521035133"></parameter>
                <parameter name="password" value="111111"></parameter>
                	<include name="testLogin"></include>
                    <exclude name="testCheck3DModelGoodsList" />
                    <exclude name="testCheck3DModelByColorAndGrade" />
                </methods>
             </class>  
        </classes>
    </test>
    
    <test name="测试退出">
    	<classes>
    		<class name="com.cmall.testcases.TestCase_Personal">
    			<methods>
    				<include name="testLogout"></include>
    			</methods>
    		</class>
    	</classes>
    </test>
    
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter" />
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
    </listeners>
</suite>
